{
  "expo": {
    "name": "CheckMate: Easy Check Splitter",
    "slug": "checkmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.itzsyboo.checkmate",
      "infoPlist": {
        "NSCameraUsageDescription": "CheckMate uses your camera to take photos of receipts so we can scan and split the bill for you.",
        "NSPhotoLibraryUsageDescription": "CheckMate lets you choose receipt images from your photo library to automatically analyze and split your bill."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5e7b3806-26d8-44c2-91b3-5634215df479"
      }
    },
    "owner": "itzsyboo"
  }
}
